<h1>USERS list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Pass</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Address</th>
            <th>City</th>
            <th>Cp</th>
            <th>Country</th>
            <th>Avatar</th>
            <th>Birthday</th>
            <th>Status</th>
            <th>Last_login</th>
            <th>Create_date</th>
            <th>Is_enable</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('users_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.EMAIL }}</td>
            <td>{{ entity.PASS }}</td>
            <td>{{ entity.FIRSTNAME }}</td>
            <td>{{ entity.LASTNAME }}</td>
            <td>{{ entity.ADDRESS }}</td>
            <td>{{ entity.CITY }}</td>
            <td>{{ entity.CP }}</td>
            <td>{{ entity.COUNTRY }}</td>
            <td>{{ entity.AVATAR }}</td>
            <td>{% if entity.BIRTHDAY %}{{ entity.BIRTHDAY|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{{ entity.STATUS }}</td>
            <td>{% if entity.LASTLOGIN %}{{ entity.LASTLOGIN|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{% if entity.CREATEDATE %}{{ entity.CREATEDATE|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{{ entity.ISENABLE }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('users_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('users_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('users_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
